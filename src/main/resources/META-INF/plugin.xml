<idea-plugin>

  <id>com.dbvis.yguard-unscrambler</id>

  <name>yGuard Unscrambler</name>

  <depends>com.intellij.modules.platform</depends>
  <depends>com.intellij.modules.lang</depends>
  <depends>com.intellij.modules.java</depends>

  <description>
    <![CDATA[
      Adds Unscrambler support for yGuard's mapping files
    ]]>
  </description>
  <change-notes>
    <![CDATA[
      <ul>
        <li><b>1.1.1</b> Updated plugin info.
        <li><b>1.0.0</b> Initial version for yGuard Unscrambler. Uses yGuard 4.0.0</li>
      </ul>
    ]]>
  </change-notes>

  <vendor url="https://github.com/dbvis/yguard-intellij-unscrambler">DbVis Software</vendor>

  <extensions defaultExtensionNs="com.intellij">
    <unscrambleSupport implementation="com.dbvis.yguard.YGuardUnscrambler"/>
  </extensions>

</idea-plugin>
